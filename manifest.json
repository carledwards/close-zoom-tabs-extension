{
    "manifest_version": 2,

    "name": "Close Zoom Tabs",
    "version": "1.0",
    "description": "Close all post-zoom tabs",

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Close Zoom Tabs",
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ]
}
